    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Edit Order</h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <form action="/admin/add-product/orders" method="post" id="forming">
              <div class="input-group mb-3">
                <span class="input-group-text" id="basic-addon1"><i class="fa-solid fa-id-badge"></i></span>
                <input name="orderId" id="nt" type="text" placeholder="order ID" class="form-control" value=""  aria-label="Username" aria-describedby="basic-addon1" >
              </div>
              <div class="form-check form-check-inline  mb-3">
        
                  <input class="form-check-input" type="radio" name="fill" id="inlineRadio1" value="filled">
            
                <label class="form-check-label" for="inlineRadio1" >Fill Order</label>
              </div>
              <div class="form-check form-check-inline  mb-3">
            
                  <input class="form-check-input" type="radio" name="fill" id="inlineRadio2" value="Nfilled" checked>
           
                <label class="form-check-label" for="inlineRadio2" >Un-Fill Order</label>
              </div>    
            </form>
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
            <button type="submit" class="btn btn-primary" form="forming">Save changes</button>
          </div>
        </div>
      </div>
    </div>
    <div class="alert alert-danger alert-dismissible fade show" role="alert" style="text-align: center; font-size: 30px; display: none;height: 110px; padding-top: 30px; " id="uni">
      Category names must be unique
      <button  style="padding-top: 44 px; display: none" type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" id="open1" ></button>
    </div>
    <div class="alert alert-danger alert-dismissible fade show" role="alert" style="text-align: center; font-size: 30px; display: none;height: 110px; padding-top: 30px; " id="uniItems">
      Item with same name & category already exists
      <button  style="padding-top: 44 px; display: none" type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" id="open1" ></button>
    </div>
   
    </div>
    <div class="alert alert-success alert-dismissible fade show" role="alert" style="text-align: center; font-size: 30px; display: none; height: 110px;  padding-top: 30px" id="suc">
      Refresh Page to view changes!
      <button style="padding-top: 44px" type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" id="open"></button>
    </div>
    
    <div class="d-flex" >
      <div class="col-md-12 p-3 scroll-row" id="big" >
        <div >
          <div id="wrapper" style="flex-direction: row;">
           <% var i =0 %>
           
          <% prod.forEach(x => { i++; %>
            <div class="card p-0" style="width: 18rem;  margin-bottom: 30px; margin-left: 20px; margin-right: 20px" id="wrapper">
              <img src="/assets/<%= x.url %>" class="card-img-top" alt="..." style="height: 350px">
              <div class="card-body">
                   <b class="card-title" style="font-size: 25px"> order-ID :</b> <%= x.orderId %> <br>
                    <% if(x.premium == true) {  %>
                      <i class="fa-solid fa-star" type="button" class="btn btn-secondary" data-bs-placement="top" title="Premium"></i><b class="card-title" style="font-size: 25px"><%= x.itemName %></b>
                  <%  } else {  %>
                      <i class="fa-solid fa-circle" type="button" class="btn btn-secondary"  data-bs-placement="top" title="Usual"></i><b class="card-title" style="font-size: 25px"><%= x.itemName %></b>
                  <% } %>
                    <br>
                    <small style="font-size: 18px"><%= x.category %></small>
                  
                  <p><%= x.userName %></p>
                  <% if(x.category == 'Whitelists') { %>
                    <b class="card-title" style="font-size: 20px">wallet:</b><%= x.wallet %>
                  <% }else { %>
                    <%=x.name %> <br>
                    <%=x.gender %><br>
                    <b class="card-title" style="font-size: 20px"> country :</b>  <%=x.country %><br>
                    <b class="card-title" style="font-size: 20px">zip-code :</b> <%=x.zipCode %><br>
                    <b class="card-title" style="font-size: 20px">house-number:</b> <%=x.houseNumber %><br>
                    <b class="card-title" style="font-size: 20px">city :</b> <%=x.city %><br>
                    <b class="card-title" style="font-size: 20px">street-name :</b> <%=x.streetName %><br>
                    <% } %>
                    <br>
                  <b class="card-title" style="font-size: 20px" >coins used :</b> <%=x.cost %>
                  <% if(x.order == true) { %>
                    <p class="card-title" style="font-size: 20px">order filled</p>
                  <% }else { %>
                    <p class="card-title" style="font-size: 20px">order not filled</p>
                    <% } %>
               </div>
            </div>
            
            <% }) %>
             

  
